{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "justtest",
          "min",
          "build",
          "prepare",
          "lint",
          "preversion",
          "typechk",
          "test",
          "watch",
          "cjscompat",
          "esmcompat",
          "compat",
          "release",
          "cleanbuild",
          "doc"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/lib"]
    }
  }
}
